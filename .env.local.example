# Vercel Postgres
# Vercel 대시보드에서 Postgres를 추가하면 자동으로 설정됩니다.
# 로컬 개발시에는 Vercel CLI로 연결하거나, 아래 값들을 직접 입력하세요.
POSTGRES_URL="your_postgres_url"
POSTGRES_PRISMA_URL="your_postgres_prisma_url"
POSTGRES_URL_NON_POOLING="your_postgres_url_non_pooling"
POSTGRES_USER="your_postgres_user"
POSTGRES_HOST="your_postgres_host"
POSTGRES_PASSWORD="your_postgres_password"
POSTGRES_DATABASE="your_postgres_database"

# Cloudinary
# https://cloudinary.com 에서 무료 계정 생성 후 아래 값들을 입력하세요.
# Dashboard > Settings > Access Keys 에서 확인 가능
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME="your_cloud_name"
CLOUDINARY_API_KEY="your_api_key"
CLOUDINARY_API_SECRET="your_api_secret"

# Cloudinary Upload Preset (선택사항)
# Settings > Upload > Upload presets 에서 unsigned 프리셋 생성
# 기본값을 사용하려면 "ml_default" 입력
CLOUDINARY_UPLOAD_PRESET="ml_default"
